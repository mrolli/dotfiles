{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#d65d0e",
          "foreground_templates": [
            "{{if contains \"root\" .UserName}}#cc241d{{ end }}"
          ],
          "properties": {
            "display_host": false
          },
          "style": "plain",
          "template": "{{ .UserName }}",
          "type": "session"
        },
        {
          "foreground": "#ebdbb2",
          "style": "plain",
          "template": " at ",
          "type": "text"
        },
        {
          "foreground": "#98971a",
          "foreground_templates": [
            "{{if .SSHSession }}#cc241d{{end}}"
          ],
          "properties": {
            "display_user": false
          },
          "style": "plain",
          "template": "{{ if .SSHSession }}\uf817 {{ end }}{{ .HostName }}",
          "type": "session"
        },
        {
          "foreground": "#ebdbb2",
          "style": "plain",
          "template": " in ",
          "type": "text"
        },
        {
          "foreground": "#d79921",
          "properties": {
            "folder_separator_icon": "/",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "\ue5ff {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#458588",
          "foreground_templates": [
            "{{ if gt .Ahead 0 }}#689d6a{{ end }}",
            "{{ if gt .Behind 0 }}#cc241d{{ end }}"
          ],
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#ebdbb2>on</> {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#b16286",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ufa1e{{ .FormattedMs }}\u2800",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#458588",
          "style": "plain",
          "template": "{{ if ne .Env.CONDA_PROMPT_MODIFIER \"false\" }}{{ .Env.CONDA_PROMPT_MODIFIER }}{{ end }}",
          "type": "text"
        },
        {
          "foreground": "#fabd2f",
          "properties": {
            "root_icon": "\u26a1"
          },
          "style": "plain",
          "template": "\uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#ebdbb2",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#fb4934{{ end }}"
          ],
          "properties": {
            "always_enabled": false,
            "display_exit_code": true
          },
          "style": "plain",
          "template": "\ue23a {{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}\uf42e{{ end }} ",
          "type": "exit"
        },
        {
          "foreground": "#ebdbb2",
          "style": "plain",
          "template": "{{if .Root}}#{{else}}${{end}}",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}} root :: {{end}}{{ .Folder }}",
  "final_space": true,
  "version": 2
}
