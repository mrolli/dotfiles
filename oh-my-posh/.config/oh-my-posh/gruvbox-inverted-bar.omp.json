{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#d65d0e",
          "background_templates": [
            "{{if contains \"root\" .UserName}}#cc241d{{ end }}"
          ],
          "foreground": "#ebdbb2",
          "leading_diamond": "\ue0b6",
          "properties": {
            "display_host": false,
            "prefix": "",
            "ssh_icon": ""
          },
          "style": "diamond",
          "type": "session"
        },
        {
          "background": "#98971a",
          "background_templates": [
            "{{if .SSHSession }}#cc241d{{end}}"
          ],
          "foreground": "#ebdbb2",
          "properties": {
            "display_user": false
          },
          "style": "diamond",
          "type": "session"
        },
        {
          "background": "#d79921",
          "foreground": "#ebdbb2",
          "properties": {
            "folder_separator_icon": "/",
            "max_depth": 3,
            "prefix": " \ue5ff ",
            "style": "agnoster_short"
          },
          "style": "diamond",
          "type": "path"
        },
        {
          "background": "#458588",
          "background_templates": [
            "{{ if gt .Ahead 0 }}#689d6a{{ end }}",
            "{{ if gt .Behind 0 }}#cc241d{{ end }}"
          ],
          "foreground": "#ebdbb2",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#b16286",
          "foreground": "#ebdbb2",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " \ufa1e{{ .FormattedMs }}\u2800",
          "trailing_diamond": "\ue0b4",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#458588",
          "foreground": "#ebdbb2",
          "leading_diamond": "\ue0b6",
          "properties": {
            "postfix": "",
            "prefix": ""
          },
          "style": "diamond",
          "template": "{{ if ne .Env.CONDA_PROMPT_MODIFIER \"false\" }}{{ .Env.CONDA_PROMPT_MODIFIER }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "text"
        },
        {
          "foreground": "#fabd2f",
          "properties": {
            "postfix": "",
            "prefix": "",
            "root_icon": "\u26a1"
          },
          "style": "plain",
          "type": "root"
        },
        {
          "background": "#cc241d",
          "leading_diamond": "\ue0b6",
          "properties": {
            "always_enabled": false,
            "color_background": false,
            "display_exit_code": true,
            "error_color": "#ebdbb2",
            "postfix": "",
            "prefix": "\ue23a "
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "exit"
        },
        {
          "foreground": "#ebdbb2",
          "properties": {
            "postfix": "",
            "prefix": " ",
            "text": "{{if .Root}}#{{else}}${{end}}"
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}} root :: {{end}}{{ .Folder }}",
  "final_space": true,
  "version": 2
}
